<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="?attr/googleButtonRippleOutline">
    <item>
        <inset
            android:insetLeft="4dp"
            android:insetRight="4dp"
            android:insetTop="6dp"
            android:insetBottom="6dp">
            <shape android:shape="rectangle">
                <corners android:radius="20dp"/>
                <solid android:color="?android:attr/windowBackground"/>
                <stroke
                    android:width="1dp"
                    android:color="?attr/googleButtonStroked"/>
                <padding
                    android:left="16dp"
                    android:top="4dp"
                    android:right="16dp"
                    android:bottom="4dp"/>
            </shape>
        </inset>
    </item>
</ripple>
